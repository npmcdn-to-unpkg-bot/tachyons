{
  "name": "tachyons",
  "version": "4.3.0",
  "description": "Functional CSS for humans",
  "author": "mrmrs",
  "style": "css/tachyons.min.css",
  "main": "css/tachyons.css",
  "files": [
    "css",
    "src"
  ],
  "repository": "tachyons-css/tachyons",
  "keywords": [
    "css",
    "oocss",
    "functional css",
    "design",
    "responsive",
    "performance"
  ],
  "license": "MIT",
  "devDependencies": {
    "copy-files": "^0.1.0",
    "immutable-css-cli": "^1.1.1",
    "normalize.css": "^4.2.0",
    "tachyons-cli": "^1.0.3",
    "tachyons-modules": "^1.1.8",
    "watch": "^0.19.2"
  },
  "contributors": [
    {
      "name": "adam morse",
      "email": "hi@mrmrs.cc"
    },
    {
      "name": "john otander",
      "url": "http://johnotander.com"
    }
  ],
  "scripts": {
    "start": "npm run build:watch",
    "mutations": "immutable-css src/tachyons.css --strict",
    "build": "npm run build:css && npm run build:minify",
    "build:css": "tachyons src/tachyons.css > css/tachyons.css",
    "build:minify": "tachyons src/tachyons.css -m > css/tachyons.min.css",
    "build:watch": "watch 'npm run build' ./src/"
  },
  "dependencies": {
    "tachyons-border-colors": "^4.1.4",
    "tachyons-border-radius": "^5.0.1",
    "tachyons-border-widths": "^3.0.1",
    "tachyons-borders": "^3.0.1",
    "tachyons-box-sizing": "^3.1.7",
    "tachyons-clears": "^3.0.1",
    "tachyons-display": "^5.0.1",
    "tachyons-floats": "^3.0.1",
    "tachyons-font-weight": "^5.0.1",
    "tachyons-heights": "^6.0.1",
    "tachyons-hovers": "^2.4.1",
    "tachyons-letter-spacing": "^3.0.1",
    "tachyons-line-height": "^3.0.1",
    "tachyons-links": "^3.0.6",
    "tachyons-max-widths": "^4.0.1",
    "tachyons-skins": "^3.2.5",
    "tachyons-spacing": "^6.0.1",
    "tachyons-text-align": "^3.0.1",
    "tachyons-text-transform": "^4.0.1",
    "tachyons-type-scale": "^6.0.1",
    "tachyons-typography": "^3.0.1",
    "tachyons-utilities": "^1.2.6",
    "tachyons-vertical-align": "^4.0.1",
    "tachyons-widths": "^5.0.1"
  }
}
